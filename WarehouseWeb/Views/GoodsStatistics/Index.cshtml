
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/MasterPage/Main.cshtml";
}

<head>
    <style>
        .mainPart {
            width: 1160px;
            background-color: white;
            border-radius: 3px;
            margin-top: 20px;
            margin-left: 15px;
            border: 1px solid #ddd;
        }

        .first, .last {
            background-color: #f9f9f9;
            border: 1px solid #ddd;
            width: 50px;
            height: 30px;
            padding: 2px 10px;
            margin-left: -5px;
        }

        .up, .next {
            background-color: #f9f9f9;
            border: 1px solid #ddd;
            width: 60px;
            height: 30px;
            padding: 2px;
            margin-left: -5px;
        }

        .PageIndex {
            background-color: #f9f9f9;
            border: 1px solid #ddd;
            width: 30px;
            height: 30px;
            padding: 2px 10px;
            margin-left: -5px;
        }

        .go {
            background-color: #f9f9f9;
            border: 1px solid #ddd;
            width: 40px;
            height: 30px;
            padding: 2px;
            margin-left: -5px;
        }
    </style>
</head>

<div class="menuBar" style="border-bottom:1px solid #ddd;">
    <div style="margin-top:10px;margin-left:30px;">
        <img src="~/Image/fangzi01-copy.png" width="13" height="10" />
        <a href="~/Warehouse/Index" style="margin-top:20px;">首页</a>
        <img src="~/Image/xiangyou.png" width="10" height="10" />
        <a href="javascript:void(0);" style="margin-top:20px;">报表管理</a>
        <img src="~/Image/xiangyou.png" width="10" height="10" />
        <a href="~/GoodsStatistics/Index" style="margin-top:20px;">货品统计</a>
    </div>
</div>
<div class="menuContent">
    <div class="mainPart">
        <div style="height:40px;background-color:#eeeeee;border-bottom:1px solid #ddd;">
            <img src="~/Image/diqiu.png" style="float: left;margin:12px 0px 10px 20px" />
            <span style="margin-top:10px;float: left;font-weight: bold;">在线库存报表</span>
        </div>
        <button class="btn btn-success" style="margin:15px 0px 15px 1055px;">导出Excel</button>
        <div class="form-inline" style="width:1130px;height:60px;border:1px solid #ddd;margin-left:15px;background-color:#f9f9f9;">
            <div style="margin:12px 10px">
                <span style="margin:0px 10px">产品/编号/条码/名称</span><input class="form-control" style="width:150px;" />
                <button class="btn btn-primary" style="margin-left:20px;width:60px">搜索</button>
            </div>
        </div>
        <div class="form-inline" style="width:1130px;margin-left:15px;">
            <hr style="margin-top:25px;" />
            <table class="table tab-content table-bordered table-hover ">
                <tr>
                    <th>产品编号</th>
                    <th>产品条码</th>
                    <th>产品名称</th>
                    <th>类别</th>
                    <th>预警值下限</th>
                    <th>预警值上限</th>
                    <th>规格</th>
                    <th>库存数</th>
                    <th>进货总数</th>
                    <th>出库总数</th>
                    <th>报损总数</th>
                </tr>
            </table>
            <div class="form-inline" style="margin: 10px 0px 0px 640px;padding-bottom: 10px;">
                <button class="first" style="background-color: #f9f9f9;border: 1px solid #ddd;width: 60px;height: 30px;padding: 2px 10px;">首页</button>
                <button class="up">上一页</button>
                <input class="PageIndex" type="text" value="1" />
                <button class="go">GO</button>
                <button class="next">下一页</button>
                <button class="last">尾页</button>
                <label>当前页是第 1 页,共计 5 条/ 1 页</label>
            </div>
        </div>
    </div>
</div>
